<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line1"
        style="@style/Guideline_horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line2"
        style="@style/Guideline_horizontal"
        app:layout_constraintGuide_percent="0.06" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line3"
        style="@style/Guideline_vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line4"
        style="@style/Guideline_horizontal"
        app:layout_constraintGuide_percent="0.12" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line5"
        style="@style/Guideline_horizontal"
        app:layout_constraintGuide_percent="0.92" />

    <View
        android:id="@+id/view_back"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/icons_8_expand_arrow"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/line2" />

    <TextView
        android:id="@+id/tv_neo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:fontFamily="@font/raleway_black"
        android:text="EDITORIAL NEWS"
        android:textColor="#121212"
        android:textSize="22sp"
        app:layout_constraintLeft_toRightOf="@id/view_back"
        app:layout_constraintTop_toTopOf="@id/view_back" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_componente_6_3"
        app:layout_constraintBottom_toBottomOf="@id/line1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/custom_layout_home_list1"
        android:paddingStart="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/line4"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/line6"
            style="@style/Guideline_vertical"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/line7"
            style="@style/Guideline_vertical"
            app:layout_constraintGuide_percent="0.95" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/line8"
            style="@style/Guideline_horizontal"
            app:layout_constraintGuide_percent="0.27" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/line9"
            style="@style/Guideline_horizontal"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_listone"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="60dp"
            app:cardCornerRadius="7dp"
            app:layout_constraintBottom_toBottomOf="@id/line8"
            app:layout_constraintLeft_toLeftOf="@id/line6"
            app:layout_constraintRight_toRightOf="@id/line7"
            app:layout_constraintTop_toTopOf="parent">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/raggruppa_16" />
        </androidx.cardview.widget.CardView>
        <TextView
            android:background="@drawable/custom_layout_new_article1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="CRYPTOCURRENCY"
            android:textColor="#0a0909"
            android:fontFamily="@font/raleway_regular"
            android:textSize="10sp"
            app:layout_constraintRight_toRightOf="@id/line7"
            app:layout_constraintTop_toTopOf="@id/line8"
            android:layout_marginRight="10dp"
            android:layout_marginTop="25dp"/>
        <View
            android:id="@+id/view_calendar"
            android:layout_width="26dp"
            android:layout_height="26dp"
            app:layout_constraintTop_toTopOf="@id/line9"
            app:layout_constraintLeft_toLeftOf="@id/line6"
            android:background="@drawable/icons_8_calendar"/>
        <TextView
            android:id="@+id/tv_setTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/view_calendar"
            app:layout_constraintLeft_toLeftOf="@id/line6"
            android:text="02 Set 2020"
            android:layout_marginTop="5dp"
            android:textSize="12sp"
            android:textColor="#000000"
            android:fontFamily="@font/lato_light"/>
        <LinearLayout
            android:padding="5dp"
            android:gravity="right"
            android:background="#753DF1"
            app:layout_constraintTop_toTopOf="@id/line9"
            app:layout_constraintRight_toRightOf="@id/line7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:textColor="#ffffff"
                android:fontFamily="@font/raleway_black"
                android:textSize="18sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="What is the future of"/>
            <TextView
                android:textColor="#ffffff"
                android:fontFamily="@font/raleway_black"
                android:textSize="18sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="cryptocurrencies?"/>
        </LinearLayout>
        <TextView
            android:id="@+id/tv_layout1"
            android:layout_width="0dp"
            android:textColor="#000000"
            android:textSize="13sp"
            android:gravity="left"
            android:fontFamily="@font/lato_regular"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tv_setTime"
            app:layout_constraintLeft_toLeftOf="@id/line6"
            app:layout_constraintRight_toRightOf="@id/line7"
            android:layout_marginTop="20dp"
            android:text="Rem deserunt voluptatem sapiente. Quod sint officiis\n quae magnam. Qui eaque atque quia. Incidunt dolor
            \n reiciendis tenetur libero error consequatur voluptate\nrecusandae. Sequi voluptatum quas. Ullam voluptatem\nreprehenderit ea commodi. Doloremque autem
            praesentium qui harum quia sunt voluptatem eius at.\nVoluptates voluptatem eaque et voluptas maxime
            \n molestiae et. Et saepe perferendis ut quidem et est. Et\niusto ut nostrum delectus. Libero et modi placeat
            \n labore sit et quaerat sed. Dolorem libero earum ipsum \n illum nemo."
            />

        <TextView
            android:textColor="#000000"
            android:textSize="13sp"
            android:gravity="left"
            android:layout_width="0dp"
            android:fontFamily="@font/lato_regular"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tv_layout1"
            app:layout_constraintLeft_toLeftOf="@id/line6"
            app:layout_constraintRight_toRightOf="@id/line7"
            android:layout_marginTop="20dp"
            android:text="Rem deserunt voluptatem sapiente. Quod sint officiis\n quae magnam. Qui eaque atque quia. Incidunt dolor\n
        reiciendis tenetur libero error consequatur voluptate \n recusandae. Sequi voluptatum quas. Ullam voluptatem\n
        reprehenderit ea commodi. Doloremque autem"
            />




    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/custom_layout_nav"
        app:layout_constraintTop_toTopOf="@id/line5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <com.google.android.material.bottomappbar.BottomAppBar
            android:id="@+id/Btnt_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:background="@color/white">

            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_NVT"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:padding="10dp"
                app:menu="@menu/menu"/>
        </com.google.android.material.bottomappbar.BottomAppBar>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</androidx.constraintlayout.widget.ConstraintLayout>