<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_marginTop="20dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_one_list2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello"
        android:textSize="18sp"
        android:fontFamily="@font/raleway_black"
        android:textColor="#676767"
        />

    <TextView
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:id="@+id/tv_two_list2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_one_list2"
        android:fontFamily="@font/lato_regular"
        android:text="hello"
        android:textColor="#4d4d4d"
        android:textSize="14sp" />

    <TextView
        android:background="@drawable/custom_radius_info2"
        android:id="@+id/tv_three_list2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:text="hello"
        android:textSize="14sp"
        android:fontFamily="@font/raleway_regular"
        android:textColor="#020202"
        />

    <TextView
        android:id="@+id/tv_four_list2"
        android:layout_below="@+id/tv_three_list2"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello"
        android:textSize="14sp"
        android:fontFamily="@font/lato_regular"
        android:textColor="#222222"
        />


</RelativeLayout>